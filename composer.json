{
    "name": "communitales/log",
    "description": "Communitales Log Component",
    "keywords": [
        "log",
        "php"
    ],
    "type": "library",
    "homepage": "https://www.communitales.com",
    "license": "MIT",
    "require": {
        "php": "^7.1.3",
        "psr/log": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5"
    },
    "autoload": {
        "psr-4": {
            "Communitales\\Component\\Log\\": "src/Log"
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Communitales\\Component\\Log\\Test\\": "test/Log"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
